func direccionFormateada (direccion: Direccion) -> String {
   let todaLaDireccion = "\(direccion)"
   return todaLaDireccion
}

print(direccionFormateada(direccion: direccion1))

print(direccionFormateada(direccion: direccion2))
print(direccionFormateada(direccion: direccion3))
